import java.util.*;

class Fraction {
	private int a;
	private int b;
	
	public int getA() {
		return a;
		}

	public int getB() {
		return b;
		}
	
	public void setAB(int a, int b) {
		this.a=a;
		if (b>0)
			this.b=b;
		}
		
	public Fraction() {
		a=1;
		b=1;
		}
		
	public Fraction(int a, int b) {
		this.a=a;
		this.b=b;
		}
	
	public Fraction(Fraction n) {
		a=n.a;
		b=n.b;
		}
	}

public class ex2 {
	public static void main (String[] args) {
		
		Fraction fra1=new Fraction();
		Fraction fra2=new Fraction(10,5);
		Fraction fra3=new Fraction();
		}		
	}